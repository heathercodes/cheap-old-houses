    "pg": "^8.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "axios": "^0.25.0",
    "babel-eslint": "^10.0.3",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^6.7.1",
    "eslint-plugin-babel": "^5.3.0",
    "nodemon": "^2.0.15"
  }
